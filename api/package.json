{
	"name": "@graph-it/api",
	"version": "1.0.0",
	"description": "The API server for the GraphIT database.",
	"scripts": {
		"build": "npm run build:app && npm run build:specs && npm run build:client",
		"build:clean": "rm -rf ../out/api",
		"build:app": "npm run barrels && tsc --project tsconfig.compile.json",
		"build:specs": "tsed run generate-swagger -o ../out/api/specs",
		"build:client": "tsed run generate-http-client -o ../out/api/client",
		"barrels": "barrelsby --config .barrelsby.json",
		"dev": "npm run barrels && tsnd --inspect --exit-child --cls --ignore-watch ../node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
		"dev:production": "NODE_ENV=production npm run barrels && NODE_ENV=production tsnd --inspect --exit-child --cls --ignore-watch ../node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
		"start": "cross-env NODE_ENV=production node ../out/api/dist/index.js",
		"test:lint": "eslint '**/*.{ts,js}'",
		"test:lint:fix": "eslint '**/*.{ts,js}' --fix"
	},
	"dependencies": {
		"@tsed/ajv": "7.14.2",
		"@tsed/cli-core": "4.6.4",
		"@tsed/cli-generate-swagger": "4.4.1",
		"@tsed/common": "7.14.2",
		"@tsed/core": "7.14.2",
		"@tsed/di": "7.14.2",
		"@tsed/engines": "7.14.2",
		"@tsed/exceptions": "7.14.2",
		"@tsed/json-mapper": "7.14.2",
		"@tsed/logger": "6.3.4",
		"@tsed/logger-file": "6.3.4",
		"@tsed/openspec": "7.14.2",
		"@tsed/platform-cache": "7.14.2",
		"@tsed/platform-exceptions": "7.14.2",
		"@tsed/platform-express": "7.14.2",
		"@tsed/platform-log-middleware": "7.14.2",
		"@tsed/platform-middlewares": "7.14.2",
		"@tsed/platform-params": "7.14.2",
		"@tsed/platform-response-filter": "7.14.2",
		"@tsed/platform-views": "7.14.2",
		"@tsed/schema": "7.14.2",
		"@tsed/swagger": "7.14.2",
		"ajv": "8.12.0",
		"barrelsby": "2.5.1",
		"body-parser": "1.20.2",
		"compression": "1.7.4",
		"config": "3.3.9",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.3",
		"dotenv-expand": "^10.0.0",
		"dotenv-flow": "^3.2.0",
		"express": "4.18.2",
		"express-session": "1.17.3",
		"method-override": "3.0.0",
		"swagger-typescript-api": "12.0.3",
		"wikibase-edit": "5.3.3"
	},
	"devDependencies": {
		"@tsed/cli-plugin-eslint": "4.2.9",
		"@types/compression": "1.7.2",
		"@types/cookie-parser": "1.4.3",
		"@types/cors": "2.8.13",
		"@types/express": "4.17.17",
		"@types/express-session": "1.17.7",
		"@types/lodash": "4.14.191",
		"@types/method-override": "0.0.32",
		"@types/multer": "1.4.7",
		"@types/node": "18.14.0",
		"@typescript-eslint/eslint-plugin": "5.53.0",
		"@typescript-eslint/parser": "5.53.0",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.1.2",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5"
	},
	"tsed": {
		"packageManager": "npm",
		"convention": "default",
		"architecture": "default"
	},
	"main": "processes.config.js",
	"author": "PDA",
	"license": "MIT"
}
